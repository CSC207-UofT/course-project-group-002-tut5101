<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UpdateFreshnessActivity">

    <Button
            android:text="Update"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button8"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="151dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="151dp" android:layout_marginBottom="175dp"/>
    <com.google.android.material.textfield.TextInputEditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:hint="Name"
            android:id="@+id/textInputEditText7" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="100dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <com.google.android.material.textfield.TextInputEditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:hint="New Freshness"
            android:id="@+id/textInputEditText8" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="38dp" app:layout_constraintTop_toBottomOf="@+id/textInputEditText7"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>