<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".inventoryactivities.UpdateQuanActivity">

    <Button
            android:text="@string/update"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button8"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="143dp" android:layout_marginBottom="291dp"/>
    <EditText
            android:layout_width="328dp"
            android:layout_height="37dp"
            android:inputType="textPersonName"
            android:text="@string/name"
            android:ems="10"
            android:id="@+id/editTextTextPersonName6"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="132dp"
            app:layout_constraintEnd_toEndOf="parent" android:autofillHints="Name" tools:ignore="LabelFor"
            app:layout_constraintHorizontal_bias="0.493"/>
    <EditText
            android:layout_width="332dp"
            android:layout_height="38dp"
            android:inputType="number|textPersonName|numberDecimal"
            android:text="usage( negative when adding)"
            android:ems="10"
            android:id="@+id/editTextTextPersonName7"
            android:layout_marginTop="56dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName6" android:layout_marginEnd="5dp"
            app:layout_constraintEnd_toEndOf="@+id/editTextTextPersonName6" android:autofillHints="Usage"
            tools:ignore="LabelFor"/>
</androidx.constraintlayout.widget.ConstraintLayout>