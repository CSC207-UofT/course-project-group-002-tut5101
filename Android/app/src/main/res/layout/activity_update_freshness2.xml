<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UpdateFreshnessActivity">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editTextTextPersonName8"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="99dp"
            android:layout_marginStart="77dp"/>
    <Button
            android:text="Update"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button11"
            app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName9"
            android:layout_marginTop="96dp" android:layout_marginStart="54dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName9"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Freshness"
            android:ems="10"
            android:id="@+id/editTextTextPersonName9"
            app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName8"
            android:layout_marginTop="41dp" android:layout_marginStart="3dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName8"/>
</androidx.constraintlayout.widget.ConstraintLayout>