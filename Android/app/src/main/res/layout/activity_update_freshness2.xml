<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".inventory_activities.UpdateFreshnessActivity">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="@string/inventory_item_name"
            android:ems="10"
            android:id="@+id/editTextTextPersonName8"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="99dp"
            android:layout_marginStart="77dp" android:autofillHints="Name" tools:ignore="LabelFor"/>
    <Button
            android:text="@string/update"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button11"
            app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName9"
            android:layout_marginTop="96dp" android:layout_marginStart="54dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName9"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="@string/freshness"
            android:ems="10"
            android:id="@+id/editTextTextPersonName9"
            app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName8"
            android:layout_marginTop="41dp" android:layout_marginStart="3dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName8" android:autofillHints="Freshness"
            tools:ignore="LabelFor"/>
</androidx.constraintlayout.widget.ConstraintLayout>